<nav>
  <ul class="pagination">
    <li class="page-item">
      <a
        class="page-link"
        href="<%=title%>/?page=<%=(--page == 0) ? 1 : page  %>"
        id="Previous">
        <span aria-hidden="true">&laquo;</span>
        <span class="sr-only">Previous</span>
      </a>
    </li>
    <% for(var i = 1; i <= soTrang;i++ ) {%>
    <li class="page-item">
      <a class="page-link" href="<%=title%>/?page=<%=i%>"><%=i%></a>
    </li>
    <% } %>
    <li class="page-item">
      <a
        class="page-link"
        href="<%=title%>/?page=<%=(++page >= soTrang) ? soTrang : ++page  %>"
        id="Next">
        <span aria-hidden="true">&raquo;</span>
        <span class="sr-only">Next</span>
      </a>
    </li>
  </ul>
</nav>
